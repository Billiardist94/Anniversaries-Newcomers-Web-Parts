{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "newcomers-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/newcomers/NewcomersWebPart.js",
          "manifest": "./src/webparts/newcomers/NewcomersWebPart.manifest.json"
        }
      ]
    },
    "anniversaries-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/anniversaries/AnniversariesWebPart.js",
          "manifest": "./src/webparts/anniversaries/AnniversariesWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ControlStrings": "./node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "NewcomersWebPartStrings": "lib/webparts/newcomers/loc/{locale}.js",
    "AnniversariesWebPartStrings": "lib/webparts/anniversaries/loc/{locale}.js"
  }
}
